import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
	    int num = sc.nextInt();
	    for(int i = 0 ; i<=5 ; i++){
	        for(int j = 0 ; j<=5 ; j++){
	            if((i == j || i+j == num-1))
	            {
	                System.out.println("*");
	            }else
	            {
	                System.out.println();
	            }
	        }
	    }
	}
}
	    
